      "files": [
        "src/cli.ts"
      ]
    }
  ],
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.21",
    "parcel-plugin-shebang": "^1.3.2",
    "typescript": "^4.1.3"
  }
}
