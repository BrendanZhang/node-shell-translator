{
  "name": "node-shell-translator",
  "version": "0.0.1",
  "main": "dist/main.js",
  "bin": {
    "fy": "dist/cli/cli.js"
  },
  "files": [
    "dist/cli/*.js"
  ],
  "scripts": {
    "dev": "ts-node-dev src/main.ts",
    "cli": "ts-node-dev src/cli.ts",
    "build": "tsc && parcel build dist/cli.js -d dist/cli"
  },
  "shebang": [
    {
      "interpreter": "node",
      "files": [
        "./dist/cli.js"
      ]
    }
  ],
  "license": "MIT",
  "dependencies": {
    "commander": "^6.2.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.21",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-shebang": "^1.3.2",
    "typescript": "^4.1.3"
  }
}
